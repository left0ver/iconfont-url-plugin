{
  "name": "iconfont-url-plugin",
  "type": "module",
  "version": "0.0.3",
  "description": "一个获取 iconfont 的在线链接并将对应的 html 标签自动注入到生成的 html 中",
  "sideEffect": false,
  "author": "left0ver",
  "license": "MIT",
  "homepage": "https://github.com/left0ver/webpack-iconfont-url-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/left0ver/webpack-iconfont-url-plugin.git"
  },
  "bugs": "https://github.com/left0ver/webpack-iconfont-url-plugin/issues",
  "keywords": [
    "iconfont",
    "iconfont-url",
    "iconfont-url-plugin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "npx tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "pnpm build",
    "release": "release-it"
  },
  "peerDependencies": {
    "html-webpack-plugin": "> 5.0.0",
    "webpack": "> 5.0.0"
  },
  "dependencies": {
    "iconfont-url": "^0.0.4"
  },
  "devDependencies": {
    "@left0ver/eslint-config-ts": "^0.0.2",
    "@types/node": "^18.11.18",
    "eslint": "^8.31.0",
    "esno": "^0.16.3",
    "html-webpack-plugin": "^5.5.0",
    "release-it": "^15.6.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
